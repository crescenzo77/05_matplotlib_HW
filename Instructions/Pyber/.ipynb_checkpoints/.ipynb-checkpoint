   "source": [
    "## Bubble Plot of Ride Sharing Data"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# Obtain the x and y coordinates for each of the three city types\n",
    "urban_type = city_ride_df[city_ride_df[\"type\"] == \"Urban\"].groupby([city_ride_df[\"city\"]])\n",
    "rural_type = city_ride_df[city_ride_df[\"type\"] == \"Rural\"].groupby([city_ride_df[\"city\"]])\n",
    "suburb_type = city_ride_df[city_ride_df[\"type\"] == \"Suburban\"].groupby([city_ride_df[\"city\"]])\n",
    "\n",
    "# Urban calculations of ride count (x), fare average(y), driver count average(s as in size)\n",
    "x_urban = urban_type[\"ride_id\"].count()\n",
    "y_urban = urban_type[\"fare\"].mean()\n",
    "s_urban = urban_type[\"driver_count\"].mean()\n",
    "\n",
    "# Rural calculations of ride count (x), fare average(y), driver count average(s as in size)\n",
    "x_rural = rural_type[\"ride_id\"].count()\n",
    "y_rural = rural_type[\"fare\"].mean()\n",
    "s_rural = rural_type[\"driver_count\"].mean()\n",
    "\n",
    "# Suburban calculations of ride count (x), fare average(y), driver count average(s as in size)\n",
    "x_suburb = suburb_type[\"ride_id\"].count()\n",
    "y_suburb = suburb_type[\"fare\"].mean()\n",
    "s_suburb = suburb_type[\"driver_count\"].mean()\n",
    "\n",
    "# Build the scatter plots for each city types\n",
    "plt.scatter(x_urban, y_urban, label = \"Urban\", s=s_urban * 8, color=[\"gold\"], edgecolor=\"orange\", alpha = 0.75, marker=\"o\")\n",
    "plt.scatter(x_rural, y_rural, label = \"Rural\", s=s_rural * 8, color=[\"skyblue\"], edgecolor=\"blue\", alpha = 0.75, marker=\"o\")\n",
    "plt.scatter(x_suburb, y_suburb, label = \"Suburb\", s=s_suburb  * 8, color=[\"coral\"], edgecolor=\"pink\", alpha = 0.75, marker=\"o\")\n",
    "plt.grid()\n",
    "\n",
    "# Incorporate the other graph properties\n",
    "plt.xlabel(\"Total # Rides per City\")\n",
    "plt.ylabel(\"Avg $ Ride Fare\")\n",
    "plt.title(\"Ride Share Data\")\n",
    "\n",
    "\n",
    "# Create a legend\n",
    "legend = plt.legend(fontsize = 8, title= \"City Types\", loc=\"best\") \n",
    "\n",
    "# Incorporate a text label regarding circle size\n",
    "plt.text(42,35,\"Note: \\nBubble size correspondes to the # of drivers per city\", fontsize = 10)\n",
    "\n",
    "# Save Figure\n",
    "plt.savefig(\"PyberRideSharingData.png\", bbox_inches=\"tight\")\n"
   ]
  },
  {
